@model IEnumerable<Feedback>
@{
    ViewData["Title"] = "Administrator Management";
    Layout = "_AdminLayout";
}


<h1>List of Feedbacks</h1>

<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">User</th>
            <th scope="col">Details</th>
            <th scope="col">Time</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Feedback item in Model)
        {
            <tr>
                <th scope="row">@item.FeedbackID</th>
                <td>@item.UserID</td>
                <td>@item.FeedbackDetails</td>
                <td>@item.DateTime</td>
            </tr>
        }
    </tbody>
</table>